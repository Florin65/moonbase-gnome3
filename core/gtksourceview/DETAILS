          MODULE=gtksourceview
         VERSION=3.20.3
          SOURCE=$MODULE-$VERSION.tar.xz
      SOURCE_URL=$GNOME_URL/sources/$MODULE/${VERSION%.*}
      SOURCE_VFY=sha256:53069c6e2645716d5dc3dc875b2fe7aacbe70e6560f8dc01c66597231886a8df
        WEB_SITE=http://www.gnome.org
         ENTERED=20030510
         UPDATED=20160509
           SHORT="Text widget that extends the gtk+-3 text widget GtkTextView"

cat << EOF
GtkSourceView is a text widget that extends the standard gtk+-3 text
widget GtkTextView. It improves GtkTextView by implementing syntax
highlighting and other features typical of a source editor.
EOF
