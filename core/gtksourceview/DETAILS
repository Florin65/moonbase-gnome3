          MODULE=gtksourceview
         VERSION=3.16.1
          SOURCE=$MODULE-$VERSION.tar.xz
      SOURCE_URL=$GNOME_URL/sources/$MODULE/${VERSION%.*}
      SOURCE_VFY=sha256:669e9846ca744ec4d04ce61377e85df7ca909ad476329922a6abf4da423f1b0c
