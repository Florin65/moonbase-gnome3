          MODULE=gnome-icon-theme-symbolic
         VERSION=3.10.1
          SOURCE=$MODULE-$VERSION.tar.xz
      SOURCE_URL=$GNOME_URL/sources/$MODULE/${VERSION%.*}
      SOURCE_VFY=sha1:5b61ee13f0a93615a7aba525438b1b6d452ba94b
        WEB_SITE=http://www.gnome.org
         ENTERED=20130706
         UPDATED=20131019
           SHORT="Symbolic icon theme for GNOME"

cat << EOF
Purpose of this icon theme is to extend the base
gnome icon theme that follows the Tango style guidelines
for speciic purposes. This would include OSD messages,
panel system/notification area, and possibly menu icons.
EOF
