          MODULE=mutter
         VERSION=3.10.2
          SOURCE=$MODULE-$VERSION.tar.xz
      SOURCE_URL=$GNOME_URL/sources/$MODULE/${VERSION::4}
      SOURCE_VFY=sha1:0df1dae82fbd41b57349adea8a30c25e377bfe94
        WEB_SITE=http://www.gnome.org
