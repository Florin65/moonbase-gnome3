          MODULE=dconf
         VERSION=0.20.0
          SOURCE=$MODULE-$VERSION.tar.xz
      SOURCE_URL=$GNOME_URL/sources/$MODULE/${VERSION%.*}
      SOURCE_VFY=sha1:638a92d176b3efa01c7b3f5dfd89029acc8d799b
        WEB_SITE=http://live.gnome.org/dconf
         ENTERED=20101128
