          MODULE=polkit-gnome
         VERSION=0.105
          SOURCE=$MODULE-$VERSION.tar.xz
      SOURCE_URL=$GNOME_URL/sources/$MODULE/$VERSION
      SOURCE_VFY=sha256:
        WEB_SITE=http://hal.freedesktop.org/releases
         ENTERED=20100227
         UPDATED=20150821
           SHORT="Authentication agent for polkit on GNOME"

cat << EOF
The polkit-gnome package provides an authentication agent for
polkit that integrates well with the GNOME desktop environment.
EOF
