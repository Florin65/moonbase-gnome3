          MODULE=libsecret
         VERSION=0.18.4
          SOURCE=$MODULE-$VERSION.tar.xz
      SOURCE_URL=$GNOME_URL/sources/$MODULE/${VERSION%.*}/
      SOURCE_VFY=sha256:0f29b51698198e6999c91f4adce3119c8c457f546b133a85baea5ea9010a19ed
