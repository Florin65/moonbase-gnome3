          MODULE=libsecret
         VERSION=0.18.2
          SOURCE=$MODULE-$VERSION.tar.xz
      SOURCE_URL=$GNOME_URL/sources/$MODULE/${VERSION%.*}/
      SOURCE_VFY=sha256:12fd288b012e1b2b1b54d586cd4c6507885715534644b4534b7ef7d7079ba443
