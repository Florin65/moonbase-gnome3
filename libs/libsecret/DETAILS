          MODULE=libsecret
         VERSION=0.20.1
          SOURCE=$MODULE-$VERSION.tar.xz
      SOURCE_URL=$GNOME_URL/sources/$MODULE/${VERSION%.*}/
      SOURCE_VFY=sha256:57f73e94ec6263a17a077fb809cf8cf424637a897a7f15b4eec42ce4aef52447
