          MODULE=libsecret
         VERSION=0.19.1
          SOURCE=$MODULE-$VERSION.tar.xz
      SOURCE_URL=$GNOME_URL/sources/$MODULE/${VERSION%.*}/
      SOURCE_VFY=sha256:8583e10179456ae2c83075d95455f156dc08db6278b32bf4bd61819335a30e3a
