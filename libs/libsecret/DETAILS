          MODULE=libsecret
         VERSION=0.20.0
          SOURCE=$MODULE-$VERSION.tar.xz
      SOURCE_URL=$GNOME_URL/sources/$MODULE/${VERSION%.*}/
      SOURCE_VFY=sha256:f1187370b453106af878e30c284a121ba0c513da8bb4170b329d66e250bdae43
